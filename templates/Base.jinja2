	<style>
	   body {
	      font-family: Arial, Helvetica, sans-serif;
       }
       table {
	      font-size: 1em;
       }
        h2 {margin-left: 50px;}
        h3 {margin-left: 60px;}
        h2a {margin-top: 10px; margin-left: 15px; font-size: 20px;}
        .admin {
            position:relative;
        }
        .admin a {
            position:absolute;
            bottom:0;
            right:0;
        }
        div#fixed {
            position: fixed;
            bottom: 0;
            right: 0;
            width: 50px;
            height: 50px;
            {#border: 3px solid red;#}
        }
    </style>

    <script src="https://code.jquery.com/jquery-3.4.1.min.js"></script>
	<script src="https://code.jquery.com/ui/1.12.1/jquery-ui.js"></script>
	<script src="https://code.jquery.com/jquery-3.4.1.min.js"></script> <!--jquery-1.12.4-->
	<script src="https://code.jquery.com/ui/1.12.1/jquery-ui.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/luxon@1.21.3/build/global/luxon.min.js"></script>

	<script>
        $(document).ready(function() {
            $('#fixed').click(function() {
                    $('.admin').slideDown("slow");
            });
        });
	    $( function() {
	        var now = luxon.DateTime.local();
	        var rezoned = now.setZone("America/Vancouver")
	        $( "#datepicker" ).datepicker({ dateFormat: 'yy-mm-dd' });
	        //$( "#datepicker" ).datepicker( "option", "gotoCurrent", true );
	    } );
	</script>
    
</head>
<body>

    {% with messages = get_flashed_messages(with_categories=true) %}
        {% if messages %}
            <ul>
               {% for message in messages %}
                  <li>{{ message[1] }}</li>
               {% endfor %}
            </ul>
        {% endif %}
    {% endwith %}


    <div class="input text" id="content">
        {% block content %}{% endblock %}
    </div>


